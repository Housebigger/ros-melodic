<?xml version="1.0"?>
<launch>
  <arg name="paused" default="false"/>
  <arg name="gazebo_gui" default="true"/>
  <arg name="urdf_path" default="$(find bigmama0306)/urdf/bigmama0306.urdf"/>

  <!-- startup simulated world -->
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find tubewhole0412)/urdf/tubewhole0416.world"/>
    <arg name="paused" value="$(arg paused)"/>
    <arg name="gui" value="$(arg gazebo_gui)"/>
  </include>

  <!-- send robot urdf to param server -->
  <param name="robot_description" textfile="$(arg urdf_path)" />

  <!-- push robot_description to factory and spawn robot in gazebo at the origin, change x,y,z arguments to spawn in a different position -->
  
<!--<node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot -x -0.000023 -y 0.000162 -z 2.203510 -R -0.001045 -P -0.001325 -Y 0.001005"
    respawn="false" output="screen" />-->
  <node name="spawn_gazebo_model" pkg="gazebo_ros" type="spawn_model" args="-urdf -param robot_description -model robot -x -0.002367 -y -0.241194 -z 2.993502 -R -3.140060 -P -0.002393 -Y 0.003674"
    respawn="false" output="screen" />

  <include file="$(find bigmama0306_moveit_config)/launch/ros_controllers.launch"/>

</launch>
